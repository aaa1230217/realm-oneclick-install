{
  "log": {
    "level": "warn",
    "output": "/var/log/realm.log"
  },
  "dns": {
    "mode": "ipv4_only",
    "protocol": "tcp_and_udp",
    "min_ttl": 600,
    "max_ttl": 3600,
    "cache_size": 256
  },
  "network": {
    "no_tcp": false,
    "use_udp": true,
    "zero_copy": true,
    "fast_open": true,
    "tcp_timeout": 5,
    "udp_timeout": 30,
    "send_proxy": false,
    "send_proxy_version": 2,
    "accept_proxy": false,
    "accept_proxy_timeout": 5
  },
  "endpoints": [
    {
      "listen": "0.0.0.0:5000",
      "remote": "1.1.1.1:443",
    },
    {
      "listen": "0.0.0.0:10000",
      "remote": "www.google.com:443",
    }
  ]
}
